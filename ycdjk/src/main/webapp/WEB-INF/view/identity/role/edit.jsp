<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>


<div class="modal-header">
	<button type="button" class="close" data-dismiss="modal" aria-hidden="true"><li class="fa fa-remove"></li></button>
	<h5 class="modal-title">新增用户</h5>
</div>

<div class="modal-body">

	<form id="role-form" name="role-form" class="form-horizontal">
		<input type="hidden" name="id" id="id">
		<input type="hidden" name="createDateTime"  id= "createDateTime" data-flag="date" data-format="yyyy-mm-dd hh:ii:ss">
		<div class="box-body">
			<div class="col-md-12">
				<div class="form-group">
					<label for="name" class="col-sm-2 control-label">名称</label>
					<div class="col-sm-9">
						<input type="text" class="form-control" id="name" name="name" placeholder="名称">
					</div>
				</div>
				
				
				<div class="form-group">
					<label for="deleted" class="col-sm-2 control-label">是否可用</label>
					<div class="col-sm-9">
						<label class="control-label"> <input type="radio" name="enable" class="square-green" checked="checked"
							value="1"> 启用
						</label> &nbsp;&nbsp;&nbsp; <label class="control-label"> <input type="radio" name="enable" class="square-green"
							value="0"> 禁用
						</label>
					</div>
				</div>
				<div class="form-group">
					<label for="remark" class="col-sm-2 control-label">说明</label>
					<div class="col-sm-9">
						<textarea class="form-control" id="remark" name="remark" placeholder="说明"></textarea>
					</div>
				</div>
			</div>
		</div>
		<!-- /.box-body -->
		<div class="box-footer text-right">
			<button type="button" class="btn btn-default" data-btn-type="cancel" data-dismiss="modal">取消</button>
			<button type="submit" class="btn btn-primary" data-btn-type="save">提交</button>
		</div>
		<!-- /.box-footer -->
	</form>

</div>
<script>
	//tableId,queryId,conditionContainer
	var form = null;
	var id= $("#roleId").val();
	$(function() {
		//初始化控件
		form=$("#role-form").form();
		//数据校验
		$("#role-form").bootstrapValidator({
			message : '请输入有效值',
			feedbackIcons : {
				valid : 'glyphicon glyphicon-ok',
				invalid : 'glyphicon glyphicon-remove',
				validating : 'glyphicon glyphicon-refresh'
			},
			submitHandler : function(validator,roleform, submitButton) {
				modals.confirm('确认保存？', function() {
					//Save Data，对应'submit-提交'
					$('#createDateTime').val(new Date().getTime());
					var params = form.getFormSimpleData();
					
					console.log(params)
					$.post(basePath+'/role/save', {'params': JSON.stringify(params)}, function(ms) {
						console.log(ms)
						if(ms.state == true){
							
							 if(id!=""){//更新
								modals.closeWin(winId);
							    roleTable.draw(); 
							}  ///新增 
								 //modals.info("数据保存成功");
							else{
								 
								 modals.closeWin(winId); 
								 roleTable.draw();
							}
						}				 
					});
				});
			},
			fields : {
				name : {
					validators : {
						notEmpty : {
							message : '请输入姓名'
						}
					}
				},
				
			}	
		});
		
		form.initComponent();
		//回填id		
		if(id!=""){
			ajaxPost(basePath+"/role/get",{'id':id},function(data){
				form.initFormData(data);
			})
		}
	});
	
	
	function resetForm(){
		form.clearForm();
        $("#role-form").data('bootstrapValidator').resetForm();
